{
    "manifest_version": 3,
    "name": "Google map comments Extractor",
    "version": "1.0",
    "description": "Extracts comments from Google Maps.",
    "permissions": ["activeTab", "scripting", "tabs", "webNavigation", "nativeMessaging"],
    "host_permissions": ["https://www.google.com/maps*" ],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "images/icons/16.png",
        "48": "images/icons/32.png",
        "128": "images/icons/128.png"
      }
    },
    "icons": {
      "16": "images/icons/16.png",
      "48": "images/icons/32.png",
      "128": "images/icons/128.png"
    }
  }